{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "training-api": {
            "projectType": "library",
            "root": "projects/training-api",
            "sourceRoot": "projects/training-api/src",
            "prefix": "crczp",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/training-api/tsconfig.lib.json",
                        "project": "projects/training-api/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/training-api/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/training-api/src/test.ts",
                        "tsConfig": "projects/training-api/tsconfig.spec.json",
                        "karmaConfig": "projects/training-api/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/training-api/**/*.ts",
                            "projects/training-api/**/*.component.html"
                        ]
                    }
                }
            }
        },
        "training-api-tester": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/training-api-tester",
            "sourceRoot": "projects/training-api-tester/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/training-api-tester",
                        "index": "projects/training-api-tester/src/index.html",
                        "browser": "projects/training-api-tester/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/training-api-tester/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "node_modules/@crczp/theme/assets",
                                "output": "assets/"
                            }
                        ],
                        "styles": [
                            "node_modules/@crczp/theme/theming/apply-all.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kB",
                                    "maximumError": "4kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "training-api-tester:build:production"
                        },
                        "development": {
                            "buildTarget": "training-api-tester:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/training-api-tester/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "node_modules/@crczp/theme/assets",
                                "output": "assets/"
                            }
                        ],
                        "styles": [
                            "node_modules/@crczp/theme/theming/apply-all.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "schematics": {
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    },
    "cli": {
        "analytics": false
    }
}
